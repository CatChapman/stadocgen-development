{
	"@context": [
		"http://www.w3.org/ns/csvw",
		{
			"@language": "en",
			"@base": "https://github.com/tdwg/ltc/source/"
		},
		{
			"csvw": "http://www.w3.org/ns/csvw#",
			"dc": "http://purl.org/dc/terms/",
			"dcterms": "http://purl.org/dc/terms/",
            "prov": "http://www.w3.org/ns/prov#",
			"rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
			"rdfs": "http://www.w3.org/2000/01/rdf-schema#",
			"schema": "http://schema.org/",
			"skos": "http://www.w3.org/2004/02/skos/core#",
			"xml": "http://www.w3.org/XML/1998/namespace/",
			"xsd": "http://www.w3.org/2001/XMLSchema#"
		}
	],
	"tableSchema": {
		"columns": [
			{
				"name": "namespace",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/namespace",
               "required": true,
             "dc:description": "Namespace prefix for the term",
             "titles": [
              "Namespace",
              "Namespace Prefix"
             ]
			},
			{
				"name": "term_local_name",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/term_local_name",
               "dc:description": "Term in lower camel casing",
              "titles": [
                  "Term"
                  ]
            },
			{
				"name": "label",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/label",
                            "dc:description": "Human-readable version of the term"
			},
			{
				"name": "definition",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/definition"
			},
			{
				"name": "usage",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/usage"
			},
			{
				"name": "notes",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/notes"
			},
			{
				"name": "examples",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/examples"
			},
			{
				"name": "rdf_type",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/rdf_type"
			},
			{
				"name": "class_name",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/class_name",
             "dc:description": "Name of the parent class (if rdf:type is property) in lower camel-casing"
			},
			{
				"name": "is_required",
				"datatype": "boolean",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/is_required",
             "dc:description": "Binary designation of whether or not a term is required"

			},
			{
				"name": "is_repeatable",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/is_repeatable"
			},
			{
				"name": "compound_name",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/compound_name",
             "dc:description": "A unique concatenated string of class_name and term_local_name "
			},
			{
				"name": "namespace_uri",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/namespace_uri"
			},
			{
				"name": "term_uri",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/term_uri"
			},
			{
				"name": "term_ns_name",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/term_ns_name",
             "dc:description": "Lower camel-casing term with the namespace prefix"
			},
			{
				"name": "datatype",
				"datatype": "string",
				"@id": "https://github.com/tdwg/ltc/source/schemas/ltc-terms-list.csv/column/datatype"
			}
		]
	},
	"url": "ltc-terms-list.csv",
	"dialect": {
		"delimiter": ",",
		"encoding": "utf-8",
		"quoteChar": "\""
